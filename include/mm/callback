#ifndef MM_CALLBACK
#define MM_CALLBACK

            PREFIX      :MM:

%
% :MM:Callback:
%
% TODO
%
% - Stable ABI,  fully implemented -
%
% This header file defines:
%   :MM:Callback:Enable (EnableG)
%   :MM:Callback:Disable (DisableG)
%

%%
% :MM:Callback:Enable
%
%   TODO
%
%   PUSHJ:
%   arg0 - timer interval in oops
%   no return value
%
Callback:Enable  IS     :MM:__CALLBACK:Enable
Callback:EnableG IS     :MM:__CALLBACK:EnableG

%%
% :MM:Callback:Disable
%
%   TODO
%
%   PUSHJ:
%   no arguments
%   no return value
%
Callback:Disable  IS    :MM:__CALLBACK:Disable
Callback:DisableG IS    :MM:__CALLBACK:DisableG


            PREFIX      :

#endif /* MM_CALLBACK */
